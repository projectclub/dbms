# Turn rewrite engine on
Options +FollowSymlinks
RewriteEngine on

# map neat URL to internal URL
RewriteRule ^ams/auth/([0-9]+)/([a-z]+)/?$   RestController.php?user=$1&pass=$2 [nc,L]
RewriteRule ^ams/courses/$    RestController.php?view=CourseAll [nc,qsa]
RewriteRule ^ams/course/([0-9]+)/$   RestController.php?view=CourseSingle&course_id=$1 [nc,qsa]